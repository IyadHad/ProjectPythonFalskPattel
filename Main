from flask import Flask, render_template, url_for,request

app = Flask(__name__,template_folder='ProjectTemplate')

@app.route("/")
@app.route("/Home")
def HomePage():
    return render_template('Home.html')

@app.route("/")
@app.route("/About")
def AboutPage():
    return render_template('about.html')

if __name__ == '__main__':
	app.run(debug=True)